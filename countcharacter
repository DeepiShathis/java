public class Main
{
    public static int count(String s, char c)
    {
        int count = 0;
        for (int i=0; i<s.length(); i++)
        {
            if (s.charAt(i) == c)
            count++;
        }
        return count;
    }
public static void main(String args[] )
{
    String s = "deepikashathis";
    char c = 'e';
    System.out.println(count(s,c));
}
}
